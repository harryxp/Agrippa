{
    "preferences": {
        "showHelpByDefault": true,
        "host": "127.0.0.1",
        "port": 3000,
        "defaultTask": { "name": "Google Search by Default"
                       , "plugin": "OnlineSearch"
                       , "url": "https://www.google.com/search?q=${q}"
                       }
    },

    "tasks": {
        "=":   { "name": "Calculator"
               , "plugin": "Calculator"
               },

        "a":   { "name": "Launch Mac Apps"
               , "plugin": "MacAppSearch"
               , "paths": ["/Applications/", "/Users/hpan/Applications/"]
               , "useFunctionKeys": false
               },
        "e":   { "name": "Launch Executables"
               , "plugin": "ExecutableSearch"
               , "paths": ["/bin/", "/usr/bin/"]
               , "useFunctionKeys": false
               },
        "f":   { "name": "Search for Files"
               , "plugin": "FileSearch"
               , "paths": ["/Users/hpan/Documents/", "/Users/hpan/Downloads/"]
               , "useFunctionKeys": false
               },

        "d":   { "name": "Dictionary"
               , "plugin": "OnlineSearch"
               , "url": "https://www.merriam-webster.com/dictionary/${q}"
               },
        "h":   { "name": "Haskell Search"
               , "plugin": "OnlineSearch"
               , "url": "https://www.google.com/search?q=haskell+${q}"
               },
        "ho":  { "name": "Home"
               , "plugin": "OnlineSearch"
               , "url": "http://peachful.space"
               },
        "pu":  { "name": "Pursuit Search"
               , "plugin": "OnlineSearch"
               , "url": "https://pursuit.purescript.org/search?q=${q}"
               },
        "py":  { "name": "Python Search"
               , "plugin": "OnlineSearch"
               , "url": "https://www.google.com/search?q=python+${q}"
               },
        "u":   { "name": "USPS Tracking"
               , "plugin": "OnlineSearch"
               , "url": "https://tools.usps.com/go/TrackConfirmAction?tLabels=${q}"
               },
        "w":   { "name": "Wikipedia Search"
               , "plugin": "OnlineSearch"
               , "url": "https://wikipedia.org/wiki/Special:Search/${q}"
               },
        "y":   { "name": "YouTube Search"
               , "plugin": "OnlineSearch"
               , "url": "http://www.youtube.com/results?search_query=${q}"
               },
        "z":   { "name": "Amazon Search"
               , "plugin": "OnlineSearch"
               , "url": "https://www.amazon.com/s?field-keywords=${q}"
               }
    }
}
