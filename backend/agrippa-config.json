{
    "preferences": {
        "showHelpByDefault": true,
        "host": "127.0.0.1",
        "port": 3000
    },

    "tasks": {
        "=":   { "task": "Calculator"
               , "plugin": "Calculator"
               },

        "'":   { "task": "Search for Files"
               , "plugin": "FileSearch"
               , "paths": ["/Users/hpan/Documents/", "/Users/hpan/Downloads/"]
               },

        "a":   { "task": "Launch Apps"
               , "plugin": "AppLauncher"
               , "paths": ["/Applications/", "/Users/hpan/Applications/"]
               },

        "d":   { "task": "Dictionary"
               , "plugin": "OnlineSearch"
               , "url": "https://www.merriam-webster.com/dictionary/${q}"
               },
        "z":   { "task": "Amazon Search"
               , "plugin": "OnlineSearch"
               , "url": "https://www.amazon.com/s?field-keywords=${q}"
               },
        "h":   { "task": "Home"
               , "plugin": "OnlineSearch"
               , "url": "http://peachful.space"
               },
        "p":   { "task": "Pursuit Search"
               , "plugin": "OnlineSearch"
               , "url": "https://pursuit.purescript.org/search?q=${q}"
               },
        "w":   { "task": "Wikipedia Search"
               , "plugin": "OnlineSearch"
               , "url": "https://wikipedia.org/wiki/Special:Search/${q}"
               },
        "y":   { "task": "YouTube Search"
               , "plugin": "OnlineSearch"
               , "url": "http://www.youtube.com/results?search_query=${q}"
               }
    }
}
