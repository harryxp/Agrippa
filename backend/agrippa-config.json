{
    "key-mappings": {
        "=":   { "plugin": "Calculator" },
        "'":   { "plugin": "FileSearch",
                 "paths": ["/Users/hpan/Documents/", "/Users/hpan/Downloads/"]
               },
        "d":   { "plugin": "OnlineSearch"
               , "url": "https://www.merriam-webster.com/dictionary/${q}"
               },
        "a":   { "plugin": "OnlineSearch"
               , "url": "https://www.amazon.com/s?field-keywords=${q}"
               },
        "pur": { "plugin": "OnlineSearch"
               , "url": "https://pursuit.purescript.org/search?q=${q}"
               },
        "wk":  { "plugin": "OnlineSearch"
               , "url": "https://wikipedia.org/wiki/Special:Search/${q}"
               },
        "yt":  { "plugin": "OnlineSearch"
               , "url": "http://www.youtube.com/results?search_query=${q}"
               }
    }
}
